extends layout

block content
	button.shiny(type='button') Shiny Button
	a.after-shine(href='#') After-Shine

	form

		label(for='#username') Username
		input(type='text',name='username',id='username',placeholder='username')

		label(for='#password') Password
		input(type='password',name='password',id='password',placeholder='password')

		button(type='submit') Submit
	
	script(src='http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.4/jquery.js')
	
	script(src='http://rawgit.com/Wisembly/basil.js/master/build/basil.js')
	
	script(src='/javascripts/test.js')